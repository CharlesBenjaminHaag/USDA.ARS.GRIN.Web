@model List<USDA.ARS.GRIN.Web.Models.PVPApplication>
<table id="table_id" class="display">
    <thead>
        <tr>
            <th>PV Number</th>
            <th>Applicant</th>
            <th>Variety (Cultivar)</th>
            <th>Expiration Date</th>
            <th>Years Protected</th>
            <th>Status</th>
            <th>Common Name</th>
            <th>Scientific Name</th>
            <th></th>

        </tr>
    </thead>
    <tbody>
        @foreach (var pVpApplication in Model)
        {
        <tr>
            <td>@pVpApplication.ApplicationNumber</td>
            <td>@pVpApplication.ApplicantName</td>
            <td>@pVpApplication.CultivarName</td>
            <td>@pVpApplication.ExpirationDate.ToShortDateString()</td>
            <td>@pVpApplication.YearsProtected</td>
            <td>@pVpApplication.ApplicationStatus</td>
            <td>@pVpApplication.CommonName</td>
            <td>@pVpApplication.ScientificName</td>
            <td>
            <a href="@Url.Action("Detail","PVP", new { id = pVpApplication.ApplicationNumber })" class="usa-button usa-button--outline">DEBUG</a>
        </tr>
        }
    </tbody>
</table>
